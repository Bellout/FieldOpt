		190	300	350	50	BHP
		210	300	350	50	BHP
		230	300	350	50	BHP
		250	300	350	50	BHP
		270	300	350	50	BHP
		290	300	350	50	BHP
		365	300	350	50	BHP
	END SCHEDULE

END WELL


!----------------------- Pipe 1 -----------------------------------
START PIPE
	NUMBER 1
	FILE pipe.dat
	OUTLETPRESSURE 20 BARA
